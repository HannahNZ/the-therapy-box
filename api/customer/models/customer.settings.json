{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "customers",
  "info": {
    "name": "customer"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "companyname": {
      "type": "string"
    },
    "billing_address": {
      "type": "component",
      "repeatable": false,
      "required": true,
      "component": "address.address"
    },
    "shipping_address": {
      "type": "component",
      "repeatable": false,
      "component": "address.address"
    },
    "orders": {
      "collection": "order",
      "via": "customer"
    },
    "zone": {
      "model": "zone"
    }
  }
}
